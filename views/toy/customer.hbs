<div class="row" style="max-width: 100%; margin:0 auto" >
{{#each toys}}
  <div class="column">
    <div class="card" >
      
      <a href="/toy/detail/{{_id}}"><img src="{{image}}" alt="Jane" style="width:330px; height:330px; margin-left: auto; margin-right: auto; margin-top:8px" ></a>
      <div class="container">
        <h6>{{name}}</h6>
        <p class="title">{{price}} $</p>
        <p><button class="button">Buy now</button></p>
      </div>
    </div>
  </div>
      {{/each}}
</div>
			<!-- ./Slider -->
    </div> <!-- End slider area -->
<!-- Include jQuery -->
<!-- Include jQuery -->
<script src="https://code.jquery.com/jquery-3.6.4.min.js"></script>
<!-- Include bxSlider -->
<script src="https://cdn.jsdelivr.net/bxslider/4.2.12/jquery.bxslider.min.js"></script>

<script>
    $(document).ready(function(){
        // Initialize the bxSlider
        $('#bxslider-home4').bxSlider({
            mode: 'fade',
            auto: true, // Tự động chuyển slide
            pause: 5000, // Thời gian giữa các slide (5 giây)
            controls: false, // Ẩn controls (prev/next buttons)
            pager: false, // Ẩn pager (dots at the bottom)
            adaptiveHeight: true
        });
    });
</script>
